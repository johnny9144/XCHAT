<!DOCTYPE html>
<html lang="zh-tw">
  <head>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="/s/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/s/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/s/animsition/css/animsition.min.css">
    <!-- inject:js -->
    <script type="text/javascript" src="/s/js/jquery.min.js"></script>
    <script type="text/javascript" src="/s/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/s/animsition/js/animsition.min.js"></script>
    <script type="text/javascript" src="/s/js/jquery.ui.min.js"></script>
    <!-- endinject -->
    <!-- bg image -->
    <style>
      body {
        margin-top: 50px;
        margin-bottom: 50px;
        background: none;
      }

      .full {
        background: url(/s/images/bg-image.jpg) no-repeat center center fixed; 
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
      }
    </style>
    <script>
      $(document).ready(function() {
        $(".animsition").animsition({
          onLoadEvent: false
        });
      });
      $(window).on('load', function() {
        $(".animsition").each(function(i){
          $(this).animsition('in');
        });
      });
    </script>
  </head>
  <body class="full">
    <!-- Page Content -->
    <div class="animsition main container-fluid">
